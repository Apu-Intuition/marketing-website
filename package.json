{
  "name": "marketing-website",
  "version": "0.1.0",
  "description": "Custom code that goes into our marketing website.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/developer-starter.git"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
